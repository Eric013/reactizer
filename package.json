{
  "name": "reactizer",
  "version": "0.8.0",
  "description": "The most hipster React boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/oreqizer/reactizer.git"
  },
  "scripts": {
    "start": "NODE_ENV=dev gulp & nodemon --exec ./node_modules/.bin/babel-node ./etc/server.dev.js",
    "server": "node ./dist/server/server.js",
    "server:tmp": "node ./.tmp/server/server.js",
    "bundle": "gulp bundle",
    "test": "jest --verbose",
    "test:watch": "jest --watch=all",
    "test:covergage": "jest --coverage",
    "lint": "eslint \"./**/*.{js,jsx}\"",
    "lint:fix": "eslint --fix \"./**/*.{js,jsx}\"",
    "messages": "gulp messages",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "native": "node node_modules/react-native/local-cli/cli.js start",
    "native:clean": "node node_modules/react-native/local-cli/cli.js start --reset-cache"
  },
  "author": "oreqizer",
  "license": "MIT",
  "engines": {
    "node": ">=5"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-polyfill": "^6.9.1",
    "chalk": "^1.1.3",
    "cookie-parser": "^1.4.3",
    "core-decorators": "^0.12.3",
    "express": "^4.14.0",
    "fs-extra": "^0.30.0",
    "github-markdown-css": "^2.3.0",
    "immutable": "^3.8.1",
    "js-cookie": "^2.1.2",
    "material-ui": "^0.15.2",
    "nconf": "^0.8.4",
    "normalize.css": "^4.2.0",
    "ramda": "^0.21.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-helmet": "^3.1.0",
    "react-intl": "^2.1.3",
    "react-native": "^0.30.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-form": "^6.0.0-rc.3",
    "redux-logger": "^2.6.1",
